---
import Layout from "@/layouts/Layout.astro";
import { UpdateProfileTabs } from "@/components/react/UpdateProfileTabs";
const { userInfo } = Astro.locals;
---

<Layout pageTitle="Actualizar Perfil">
  <section class="max-w-7xl w-full mx-auto py-10">
    <UpdateProfileTabs
      client:visible
      initialName={userInfo?.userName || ""}
      initialEmail={userInfo?.userEmail || ""}
    />
  </section>
</Layout>

<style scoped>
  @import "tailwindcss";
</style>
